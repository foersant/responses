{
  "name": "@iqb/responses",
  "author": "IQB - Institut zur QualitÃ¤tsentwicklung im Bildungswesen",
  "license": "CC-BY-SA-4.0",
  "description": "Data interfaces for processing assessment data.",
  "scripts": {
    "test_codingResults": "jest test/test-codingResults.test.ts",
    "test_response": "ts-node test/test-jsonSchemas.ts response",
    "test_coding_scheme": "ts-node test/test-jsonSchemas.ts coding-scheme",
    "test_coding": "cd test && ts-node test-coding case2",
    "test_coding2text": "cd test && ts-node test-code2text case2",
    "test_getBaseVarsList": "cd test && ts-node test-getBaseVarsList get-base-vars-case",
    "validate_coding_scheme": "cd test && ts-node validate-coding-scheme validate-coding-scheme-case",
    "generate_docs": "asyncapi generate fromTemplate json_schema/asyncapi.yaml @asyncapi/html-template -o ./public --force-write && cat json_schema/add.css >> ./public/css/global.min.css",
    "prepare_publish": "rm -rf dist/* && tsc --module commonjs && cp ./package_npm.json ./dist/package.json && cp ./README.md ./dist",
    "npm_publish": "cd dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iqb-berlin/responses.git"
  },
  "bugs": {
    "url": "https://github.com/iqb-berlin/responses/issues"
  },
  "homepage": "https://github.com/iqb-berlin/responses#readme",
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "*.ts"
        ],
        "extends": "@iqb/eslint-config"
      },
      {
        "files": [
          "*.js"
        ],
        "extends": "@iqb/eslint-config/javascript"
      }
    ]
  },
  "devDependencies": {
    "@asyncapi/cli": "^1.7.3",
    "@asyncapi/html-template": "^2.2.2",
    "@babel/preset-typescript": "^7.24.1",
    "@iqb/eslint-config": "^2.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.4.4",
    "ajv": "^8.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0"
  }
}
